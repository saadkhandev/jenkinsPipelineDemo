pipeline {
    agent any

    stages {
        stage('Saad') {
            steps {
                echo 'Hello World'
            }
        }
        
         stage('Build') {
            steps {
                echo 'building'
            }
        }
        
         stage('Deploy') {
            steps {
                echo 'deploying'
            }
        }
        
         stage('Test') {
            steps {
                echo 'Testing'
            }
        }
    }
}
